{
  "name": "easy-test-coverage-GPT",
  "displayName": "Easy Test Coverage GPT",
  "publisher": "yuriikyry4enko7",
  "description": "This extension generates unit tests for C# code using OpenAI's GPT-3 API.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.76.0"
  },
  "activationEvents": [
    "onCommand:easy-test-coverage-GPT.covertWithTestCommand",
    "onCommand:copySelectionCommand"
  ],
  "commands": [
    {
      "command": "easy-test-coverage-GPT.covertWithTestCommand",
      "title": "Covert With Tests"
    }
  ],
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "title": "Easy Test Coverage GPT",
      "type": "object",
      "properties": {
        "easy-test-coverage-GPT.openai-api-key": {
          "type": "string",
          "default": ""
        }
      }
    },
    "commands": [
      {
        "command": "easy-test-coverage-GPT.covertWithTestCommand",
        "title": "Covert with UnitTests",
        "category": "Extension Tests"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorTextFocus",
          "command": "easy-test-coverage-GPT.covertWithTestCommand",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "openai-api": "^1.3.1"
  }
}
